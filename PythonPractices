class Numbers:

    def _init_(self):
        self.Numbers = []

    def range(self):
        print("Lista de Numeros")
        for num in range (1,20):
        print(self.Numbers[num])

            if self.Numbers[num]%15 == 0:
                print ("FIZZBUZZ")
            elif self.Numbers[num]%5 == 0:
                print ("BUZZ")
            elif self.Numbers[num]%3 == 0:
                print ("FIZZ")
            else:
                print(self.Numbers[num])
